function out = simtorobot

    global inputsim servo f

    resol = [(300*pi/180)/1024 (114*2*pi/60)/1024; %ax
             (360*pi/180)/4096 (116.62*2*pi/60)/1024]; %mx %rad e rad/s

    simaux = inputsim(:,2:end);
    vel = diff(simaux)*f; %velocidade em rad/s

    for i = 1:12
            outpos(:,servo(1,i)) = round(simaux(:,i)/resol(servo(2,i),1));
            outvel(:,servo(1,i)) = ceil(abs(vel(:,i)/resol(servo(2,i),2)));
    end

    outvel = [zeros(1,12); outvel];

    

        
end